    echo -en ";5;033m\e[38;5;033m▄\e[0m\e[0m\e[48;5;033m\e[38;5;033m▄"
    echo -en "\e[0m\e[0m\e[48;5;232m\e[38;5;033m▄\e[0m\e[0m\e[48;5;03"
    echo -en "3m\e[38;5;033m▄\e[0m\e[0m\e[48;5;033m\e[38;5;033m▄\e[0m"
    echo -en "\e[0m\e[48;5;033m\e[38;5;033m▄\e[0m\e[0m\e[48;5;033m\e["
    echo -en "38;5;033m▄\e[0m\e[0m\e[48;5;033m\e[38;5;033m▄\e[0m\e[0m"
    echo -en "\e[48;5;033m\e[38;5;033m▄\e[0m\e[0m\e[48;5;232m\e[38;5;"
    echo -en "033m▄\e[0m\e[0m\e[38;5;232m▄\e[0m"
    echo
    echo -en " \e[48;5;232m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5"
    echo -en ";232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[4"
    echo -en "8;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m"
    echo -en "▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;0"
    echo -en "33m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0"
    echo -en "m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e"
    echo -en "[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0"
    echo -en "m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5"
    echo -en ";232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[4"
    echo -en "8;5;232m\e[38;5;232m▄\e[0m\e[0m      \e[48;5;232m\e[38;"
    echo -en "5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e["
    echo -en "48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232"
    echo -en "m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;"
    echo -en "033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e["
    echo -en "0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m"
    echo -en "\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e"
    echo -en "[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38"
    echo -en ";5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e"
    echo -en "[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;23"
    echo -en "2m▄\e[0m\e[0m\e[48;5;232m\e[38;5;232m▄\e[0m\e[0m"
    echo
    echo
}

read_resume_file() {
    host="$(json_get "host")"
    local ignore
    while read -r ignore; do
        ignores+=("$ignore")
    done < <(json_get "ignores[]"); unset ignore
    simulate="$(json_get "simulate")"
    tls="$(json_get "tls")"
    local wordlist
    while read -r wordlist; do
        wordlists+=("$wordlist")
    done < <(json_get "wordlists[]"); unset wordlist
}

# summarize_results() {
#     [[ -n $summarize ]] || return
#     [[ -f "$results" ]] || return
#
#     info "Summarizing results"
#     while read -r result; do
#         good "${result:4}"
#     done < <(grep -oPs "\[\+\].+" $results | sort -u); unset result
# }

usage() {
    [[ $1 -ne 0 ]] || logo >&2
    cat <<EOF
Usage: ${0##*/} [OPTIONS] <host/ip>[:port][/path][?query]

Fuzz paths on a webserver. By default, MegaBuster will try to use the
directory-list-2.3-medium.txt list from DirBuster and the big.txt and
common.txt lists from dirb.

Options:
    -a, --apend          Append user specified lists to the default
    -c, --cache          Cache locations of defaults lists
    -h, --help           Display this help message
    -i, --ignore=CODE    Ignore specified return codes (can be used
                         more than once)
    -l, --list=LIST      Use the specified wordlist (can be used more
                         than once)
    --nocolor            Disable colorized output
    -p, --proxy=PROXY    Proxy traffic through PROXY
    -r, --resume         Resume from a previous run
    --simulate           Simulate output but do nothing
    -t, --threads=NUM    Use specied number of threads (default: 32)
    --timeout=SECS       Use specified curl timeout (default: none)
    --tls                Use TLS connection (https)
    -v, --verbose        Do not hide parallel errors

EOF
    exit "$1"
}

write_resume_file() {
    [[ -n $cache ]] || return
    [[ -n $json ]] || return
    rm -rf "$cache"
    mkdir -p "$cache"

    cat >"$json" <<EOF
{
  "host": "${host:-${simulate:+simulation}}",
  "ignores": [
$(
    for i in $(seq 1 ${#ignores[@]}); do
        echo "    \"${ignores[$i - 1]}\"${ignores[$i]:+,}"
    done; unset i
)
  ],
  "simulate": "$simulate",
  "tls": "$tls",
  "wordlists": [
$(
    for i in $(seq 1 ${#wordlists[@]}); do
        echo "    \"${wordlists[$i - 1]}\"${wordlists[$i]:+,}"
    done; unset i
)
  ]
}
EOF
}

declare -a args deps ignores wordlists
unset append docache help proxy resume simulate timeout tls verbose
color="true"
curl="curl -IkLs"
deps+=("curl")
deps+=("jq")
deps+=("parallel")
threads="32"

# Check for missing dependencies
check_deps

# Parse command line options
while [[ $# -gt 0 ]]; do
    case "$1" in
        "--") shift && args+=("$@") && break ;;
        "-a"|"--append") append="true" ;;
        "-c"|"--cache") docache="true" ;;
        "-h"|"--help") help="true" ;;
        "-i"|"--ignore"*) ignores+=("$(long_opt "$@")") || shift ;;
        "-l"|"--list"*) wordlists+=("$(long_opt "$@")") || shift ;;
        "--nocolor") unset color ;;
        "-p"|"--proxy"*) proxy="--proxy $(long_opt "$@")" || shift ;;
        "-r"|"--resume") resume="--resume" ;;
        "--simulate") simulate="true" ;;
        "-t"|"--threads"*) threads="$(long_opt "$@")" || shift ;;
        "--timeout"*) timeout="-m $(long_opt "$@")" || shift ;;
        "--tls") tls="true" ;;
        "-v"|"--verbose") verbose="true" ;;
        *) args+=("$1") ;;
    esac
    shift
done
[[ ${#args[@]} -eq 0 ]] || set -- "${args[@]}"

# Check for valid params
[[ -z $help ]] || usage 0
[[ $# -le 1 ]] || usage 1
[[ $# -eq 1 ]] || [[ -n $docache$simulate ]] || usage 1

# Cite if needed
if [[ ! -f "$HOME/.parallel/will-cite" ]]; then
    mkdir -p "$HOME/.parallel"
    parallel --citation
fi

# Save host
host="${1:-${simulate:+simulation}}"

# Cache variables
hash="$(echo -n "$host" | sha256sum | awk '{print $1}')"
cache="$HOME/.cache/${0##*/}/$hash"
joblog="$cache/joblog.txt"
json="$cache/${0##*/}.json"

# If defaults lists are not cached, find and cache
dcache="$HOME/.cache/${0##*/}/default_list.locations"
[[ -f "$dcache" ]] || docache="true"
[[ -z $docache ]] || cache_default_lists
[[ -n $host ]] || exit

# Ensure host is reachable
if [[ -z $simulate ]]; then
    ping -c 1 -W 1 "${host%%[:/]*}" &>/dev/null
    [[ $? -eq 0 ]] || errx 3 "Target is out of range"
fi

# Use default lists if none provided or append was specified
if [[ ${#wordlists[@]} -eq 0 ]] || [[ -n $append ]]; then
    while read -r wordlist; do
        wordlists+=("$wordlist")
    done <"$dcache"; unset wordlist
fi

# Ensure lists are provided and valid
[[ ${#wordlists[@]} -ne 0 ]] || errx 4 "No wordlists provided"
for wordlist in "${wordlists[@]}"; do
    [[ -f "$wordlist" ]] || errx 5 "$wordlist does not exist"
done; unset wordlist

# Lock to prevent parallel issues
check_lock

# Check for resume file
check_resume_file
hide_cursor

[[ -z $resume ]] || info "Resuming..."

# Display logo
logo >&2

info "MegaBuster is targeting $host"
info "Processing wordlists for ammo"

total="0"
while read -r count; do
    ((total += count))
done < <(grep -chPvs "^( *#.*)?$" "${wordlists[@]}"); unset count
[[ $total -gt 0 ]] || errx 2 "No ammo"

subinfo "$total shots to fire"

# Create sub-script
script="/tmp/${0##*/}.parallel"
cat >"$script" <<EOF
#!/usr/bin/env bash

### Helpers begin
set -o noglob
err() { echo -e "\r${color:+\e[31m}[!] \$*\e[0m\e[K"; }
errx() { echo -e "\r${color:+\e[31m}[!] \${*:2}\e[0m\e[K"; exit \$1; }
good() { echo -e "\r${color:+\e[32m}[+] \$*\e[0m\e[K"; }
info() { echo -e "\r${color:+\e[37m}[*] \$*\e[0m\e[K"; }
subinfo() { echo -e "\r${color:+\e[36m}[=] \$*\e[0m\e[K"; }
warn() { echo -e "\r${color:+\e[33m}[-] \$*\e[0m\e[K"; }
### Helpers end

check() {
  # Check GET and POST
  local data line type
  for type in GET POST; do
    case "\$type" in
      "POST") data="{}" ;;
      *) unset data ;;
    esac
    while read -r line; do
      case "\$line" in
        404) continue ;;
        ${ignores:+$(join_with "|" "${ignores[@]}")) continue ;;}
        5*) err "\$type /\$1 (\$line)" ;;
        4*) warn "\$type /\$1 (\$line)" ;;
        3*) subinfo "\$type /\$1 (\${line/http*:\/\/$host/})" ;;
        *) good "\$type /\$1 (\$line)" ;;
      esac
    done < <(
      $curl \${data:+-d \$data} $proxy $timeout -X \$type \\
      "http${tls:+s}://$host/\$1" | \\
      awk '/^(HTTP|Location)/ {print \$2}' | tr -d "\r" | \\
      sed -r "\\\$!N;s/\n/ /"
    ); unset line
  done; unset type
}

simulate() {
  local ret type
  case "\$((RANDOM % 2))" in
    0) type="GET" ;;
    *) type="POST" ;;
  esac
  case "\$((RANDOM % 6))" in
    0) ret="200" ;;
    1) ret="301" ;;
    2) ret="302" ;;
    3) ret="401" ;;
    4) ret="403" ;;
    *) ret="500" ;;
  esac
  case "\$((RANDOM % 64))" in
    8)
      case "\$ret" in
        404) return ;;
        ${ignores:+$(join_with "|" "${ignores[@]}")) return ;;}
        5*) err "\$type /\$1 (\$ret)" ;;
        4*) warn "\$type /\$1 (\$ret)" ;;
        3*) subinfo "\$type /\$1 (\$ret /some/other/path)" ;;
        *) good "\$type /\$1 (\$ret)" ;;
      esac
      ;;
  esac
}

# Ignore comments
comment_regex="^( *#.*)?$"
[[ ! "\$1" =~ \$comment_regex ]] || exit

# Remove leading slashes and trailing ^M
path="\${1#\${1%%[^/]*}}"
path="\${path%}"

# Fix any other ^M
path="\${path///\\\\\\\r}"

if [[ "x$simulate" != "x" ]]; then
  simulate "\$path"
else
  check "\$path"
fi
EOF
chmod 700 "$script"

cat "${wordlists[@]}" | parallel --bar --joblog ${resume:++}"$joblog" \
    --lb -P "$threads" -r $resume "$script" {}
[[ -n $verbose ]] || echo -en "\e[1A\e[K" >&2

clean
