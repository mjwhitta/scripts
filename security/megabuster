long_opt() {
    local arg shift="0"
    case "$1" in
        "--"*"="*) arg="${1#*=}"; [[ -n $arg ]] || usage 1 ;;
        *) shift="1"; shift; [[ $# -gt 0 ]] || usage 1; arg="$1" ;;
    esac
    echo "$arg"
    return $shift
}

read_resume_file() {
    host="$(cat $json | jq -cMrS ".host" | sed "s/null//")"
    rround="$(cat $json | jq -cMrS ".round" | sed "s/null//")"
    simulate="$(cat $json | jq -cMrS ".simulate" | sed "s/null//")"
    tls="$(cat $json | jq -cMrS ".tls" | sed "s/null//")"
}

usage() {
    [[ $1 -ne 0 ]] || logo
    echo "Usage: ${0/*\//} [OPTIONS] <host/ip>[:port][/path][?query]"
    echo
    echo "Simple bash script to enumerate directories and files on a"
    echo "webserver. MegaBuster is like Mega Man, DirBuster and dirb"
    echo "had a baby!"
    echo
    echo "Options:"
    # echo "    -d, --directories    Only search for directories"
    # echo "    -f, --files          Only search for files"
    echo "    -h, --help           Display this help message"
    echo "    -l, --list=LIST      Use the specified wordlist"
    echo "                         (default: $list"
    echo "                         from $dirbuster)"
    echo "    --nocolor            Don't colorize output"
    # echo "    -p, --parallel       Run using parallel (if installed)"
    echo "    -r, --resume         Resume from a previous run"
    echo "    -s, --simulate       Temporary for testing, simulate"
    echo "                         output but do nothing"
    local default="(default: $threads)"
    echo "    -t, --threads=NUM    Use the specied number of threads"
    echo "                         if running in parallel $default"
    echo "    --timeout=SECS       Use specified timeout with curl"
    echo "                         (default: none)"
    echo "    --tls                Use TLS connection (https)"
    echo
    exit $1
}

warn() { echo -e "${clr:+\e[33m}[-] $@\e[0m"; }

write_resume_file() {
    cat >$json <<EOF
{
  "host": "$host",
  "round": "${1:-0}",
  "simulate": "$simulate",
  "tls": "$tls"
}
EOF
}

declare -a args
unset help resume simulate timeout tls wordlist
cache="megabuster.resume"
clr="true"
dcache="$HOME/.cache/megabuster/dirbuster.list"
# directories="true"
# files="true"
joblog="$cache/joblog.txt"
json="$cache/megabuster.json"
parallel="true"
threads="32"

# Create dcache directory
mkdir -p $(dirname $dcache)

# Check cache first
list="directory-list-2.3-medium.txt"
if [[ -f $dcache ]]; then
    wordlist="$(cat $dcache)"
    dirbuster="$(dirname $wordlist)"
else
    # If not cached, find and cache

    # Find dirbuster directory
    dirbuster="$(
        find -O3 /usr -maxdepth 4 -name "dirbuster" -type d -print \
        -quit 2>/dev/null
    )"

    # If dirbuster was found, find the list
    if [[ -n $dirbuster ]]; then
        wordlist="$(
            find $dirbuster -name "$list" -type f -print -quit \
            2>/dev/null
        )"

        # Cache location of list
        echo "$wordlist" >$dcache
    fi
fi

while [[ $# -gt 0 ]]; do
    case "$1" in
        "--") shift && args+=("$@") && break ;;
        # "-d"|"--directories") unset files ;;
        # "-f"|"--files") unset directories ;;
        "-h"|"--help") help="true" ;;
        "-l"|"--list"*) wordlist="$(long_opt $@)" || shift ;;
        "--nocolor") unset clr ;;
        # "-p"|"--parallel") parallel="true" ;;
        "-r"|"--resume") resume="--resume" ;;
        "-s"|"--simulate") simulate="true" ;;
        "-t"|"--threads"*) threads="$(long_opt $@)" || shift ;;
        "--threads"*)
            arg="$(long_opt $@)" || shift
            timeout="--connect-timeout $arg --max-time $arg"
            ;;
        "--tls") tls="true" ;;
        *) args+=("$1") ;;
    esac
    shift
done
[[ -z ${args[@]} ]] || set -- "${args[@]}"

# Check for valid params and missing dependencies
[[ -z $help ]] || usage 0
[[ $# -eq 1 ]] || [[ -n $resume ]] || usage 2
for dep in curl jq; do
    [[ -n $(command -v $dep) ]] || err 3 "$dep is not installed"
done; unset dep
[[ -f $wordlist ]] || err 4 "Wordlist does not exist"
# if [[ -z $directories ]] && [[ -z $files ]]; then
#     err 5 "Not searching for files? Not searching for directories?"
# fi

# Determine parallel situation
if [[ -n $parallel ]] && [[ -z $(command -v parallel) ]]; then
    warn "Parallel is not installed, using slow uncached behavior"
    unset parallel
fi

# Cite if needed
if [[ -n $parallel ]] && [[ ! -f $HOME/.parallel/will-cite ]]; then
    mkdir -p $HOME/.parallel
    parallel --citation
fi

host="$1"
round="0"

# Resume functionality only works with parallel
if [[ -n $parallel ]]; then
    # Ensure resume directory exists
    mkdir -p $cache

    # Check for resume file
    if [[ -f $json ]] && [[ -z $resume ]]; then
        warn "Resume file found"
        while :; do
            # Prompt whether to overwrite or resume
            read -n 1 -p "Would you like to resume [y/n/q]: " -s ans
            echo

            case "$ans" in
                "n") write_resume_file; break ;;
                "q") exit 0 ;;
                "y") read_resume_file; resume="--resume"; break ;;
                *) echo "Invalid response, try again!" ;;
            esac
        done
    elif [[ -f $json ]]; then
        read_resume_file
    elif [[ -n $resume ]]; then
        err 6 "No resume file found in current directory"
    else
        write_resume_file
    fi
fi

# Display logo
logo

# Script used by parallel
script="/tmp/${0/*\//}.parallel"
trap clean SIGINT

cat >$script <<EOF
#!/usr/bin/env bash

check() {
    data="\$(
        curl $timeout -Iks "http${tls:+s}://$host/\$1" | \\
        awk '/^HTTP\// {print \$2}'
    )"
    if [[ -n \$data ]] &&
       [[ \$(echo "\$data" | head -n 1) -ne 404 ]]
    then
        info "\$1 (\$data)"
    fi
}

# err() { echo -e "\r${clr:+\e[31m}[!] \${@:2}\e[0m\e[K"; exit \$1; }

info() { echo -e "\r${clr:+\e[32m}[+] \$@\e[0m\e[K"; }

# warn() { echo -e "\r${clr:+\e[33m}[-] \$@\e[0m\e[K"; }

if [[ "x$simulate" != "x" ]]; then
    let "rand = RANDOM % 50"
    [[ \$rand -ne 7 ]] || info "\$1"
else
    check "\$1"
fi
EOF
chmod 700 $script

info "Processing dataset for jobs"

declare -a dataset=($(cat $wordlist | grep -Ev "^( *#.*)?$"))
let "total = ${#dataset[@]}"

[[ $total -ne 0 ]] || err 7 "No dataset provided"

if [[ -n $parallel ]]; then
    # Math
    left="$total"
    marker="0"
    maxdata="$(
        parallel -r --show-limit </dev/null | \
        awk '/Maximal used/ {print $NF; exit}'
    )"
    [[ $maxdata -lt 5000 ]] || let "maxdata -= 5000"
    let "remainder = total % maxdata"
    let "rounds = total / maxdata"
    [[ $remainder -eq 0 ]] || let "rounds += 1"

    # Loop thru rounds
    info "$total jobs to run"
    [[ $rounds -eq 1 ]] || info "Splitting into $rounds rounds"
    while [[ $left -gt 0 ]]; do
        let "round += 1"
        [[ $round == $rround ]] && unset rround

        increment="$left"
        [[ $left -lt $maxdata ]] || let "increment = $maxdata - 1"

        if [[ -z $rround ]]; then
            write_resume_file $round

            [[ $rounds -eq 1 ]] || warn "Round $round"

            parallel --bar --joblog ${resume:++}$joblog --lb \
                -P $threads -r $resume \
                $script {} ::: ${dataset[@]:$marker:$increment}
            unset resume
        fi

        let "left -= increment"
        let "marker += increment"
        [[ -n $rround ]] || echo -en "\e[1A\e[K"
    done
else
    let "count = 1"
    for data in "${dataset[@]}"; do
        echo -en "\r[$count/$total]\e[K" >&2
        $script "$data"
        let "count += 1"
    done; unset data
fi

rm -rf $cache
clean
