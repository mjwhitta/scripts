### Helpers begin
set -o noglob
trap "clean 128" SIGINT # Ensure cleanup on ^C
check_deps() {
    for d in "${deps[@]}"; do
        [[ -n $(command -v "$d") ]] || errx 127 "$d is not installed"
    done; unset d
}
check_lock() {
    [[ -n $cache ]] || errx 126 "Cache is not defined"
    mkdir -p "$cache"
    if [[ -f "$cache.lock" ]]; then
        errx 125 "$cache.lock already exists"
    fi
    echo "$$" >"$cache.lock"
}
check_resume_file() {
    if [[ -f "$json" ]] && [[ -z $resume ]]; then
        warn "Resume file found" >&2
        while :; do
            # Prompt whether to overwrite or resume
            local a
            read -n 1 -p "Would you like to resume [y/n/q/c]: " -rs a
            echo

            case "$a" in
                "c") clean 0 ;;
                "n") write_resume_file; break ;;
                "q") exit 0 ;;
                "y") read_resume_file; resume="--resume"; break ;;
                *) echo "Invalid response, try again!" ;;
            esac
        done
    elif [[ -f "$json" ]]; then
        read_resume_file
    elif [[ -n $resume ]]; then
        errx 124 "No resume file found"
    else
        write_resume_file
    fi
}
clean() {
    if [[ ${1:-0} -eq 0 ]]; then
        [[ -z $cache ]] || [[ ! -d "$cache" ]] || rm -rf "$cache"
    fi
    [[ -z $script ]] || [[ ! -f "$script" ]] || rm -f "$script"
    [[ ${1:-0} -eq 125 ]] || [[ -z $cache ]] || rm -f "$cache.lock"
    show_cursor
    exit "${1:-0}"
}
err() { echo -e "${color:+\e[31m}[!] $*\e[0m"; }
errx() { echo -e "${color:+\e[31m}[!] ${*:2}\e[0m"; clean "$1"; }
good() { echo -e "${color:+\e[32m}[+] $*\e[0m"; }
hide_cursor() { echo -en "\e[?25l"; }
info() { echo -e "${color:+\e[37m}[*] $*\e[0m"; }
json_get() {
    if [[ -z $json ]] || [[ ! -f "$json" ]]; then
        return
    fi
    jq -cr ".$*" "$json" | sed -r "s/^null$//g"
}
long_opt() {
    local arg shift="0"
    case "$1" in
        "--"*"="*) arg="${1#*=}"; [[ -n $arg ]] || usage 123 ;;
        *) shift="1"; shift; [[ $# -gt 0 ]] || usage 123; arg="$1" ;;
    esac
    echo "$arg"
    return $shift
}
show_cursor() { echo -en "\e[?25h"; }
subinfo() { echo -e "${color:+\e[36m}[=] $*\e[0m"; }
warn() { echo -e "${color:+\e[33m}[-] $*\e[0m"; }
### Helpers end

cache_default_lists() {
    # Create dcache directory
    mkdir -p "$(dirname "$dcache")"
    rm -f "$dcache"

    local dirb_blist="big.txt"
    local dirb_clist="common.txt"
    local dirbuster_list="directory-list-2.3-medium.txt"
    local list wordlist

    # Find dirb directory
    local dirb="$(
        find -O3 /usr -maxdepth 4 -name "dirb" -type d -print -quit \
        2>/dev/null
    )"

    # If dirb was found, find the lists
    if [[ -n $dirb ]]; then
        for list in $dirb_blist $dirb_clist; do
            wordlist="$(
                find "$dirb" -name "$list" -type f -print -quit \
                2>/dev/null
            )"

            # Cache location of list
            [[ -z $wordlist ]] || echo "$wordlist" >>"$dcache"
        done; unset list
    fi

    # Find dirbuster directory
    local dirbuster="$(
        find -O3 /usr -maxdepth 4 -name "dirbuster" -type d -print \
        -quit 2>/dev/null
    )"

    # If dirbuster was found, find the list
    if [[ -n $dirbuster ]]; then
        wordlist="$(
            find "$dirbuster" -name "$dirbuster_list" -type f -print \
            -quit 2>/dev/null
        )"

        # Cache location of list
        [[ -z $wordlist ]] || echo "$wordlist" >>"$dcache"
    fi

    touch "$dcache"
}

join_with() { local IFS="$1"; echo "${*:2}"; }

