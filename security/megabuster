    echo -en "m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;"
    echo -en "033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e["
    echo -en "0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m"
    echo -en "\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e"
    echo -en "[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38"
    echo -en ";5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e"
    echo -en "[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;23"
    echo -en "2m▄\e[0m\e[0m\e[48;5;232m\e[38;5;232m▄\e[0m\e[0m"
    echo
    echo
}

long_opt() {
    local arg shift="0"
    case "$1" in
        "--"*"="*) arg="${1#*=}"; [[ -n $arg ]] || usage 1 ;;
        *) shift="1"; shift; [[ $# -gt 0 ]] || usage 1; arg="$1" ;;
    esac
    echo "$arg"
    return $shift
}

read_resume_file() {
    host="$(json_get "host")"
    simulate="$(json_get "simulate")"
    tls="$(json_get "tls")"
    wordlists=($(json_get "wordlists[]"))
}

subinfo() { echo -e "${color:+\e[36m}[=] $@\e[0m"; }

# summarize_results() {
#     [[ -n $summarize ]] || return
#     [[ -f $results ]] || return

#     info "Summarizing results"
#     while read -r result; do
#         good "${result:4}"
#     done < <(grep -Eos "\[\+\].+" $results | sort -u); unset result
# }

usage() {
    [[ $1 -ne 0 ]] || logo >&2
    echo "Usage: ${0/*\//} [OPTIONS] <host/ip>[:port][/path][?query]"
    echo
    echo "Simple bash script to enumerate directories and files on a"
    echo "webserver. MegaBuster is like Mega Man, DirBuster and dirb"
    echo "had a baby! By default, MegaBuster will try to use the"
    echo "big.txt and common.txt lists from dirb as well as the"
    echo "directory-list-2.3-medium.txt list from DirBuster."
    echo
    echo "Options:"
    echo "    -a, --apend          Append user specified lists to"
    echo "                         the default"
    echo "    -c, --cache          Cache locations of defaults lists"
    echo "    -h, --help           Display this help message"
    echo "    -l, --list=LIST      Use the specified wordlist (can be"
    echo "                         used more than once)"
    echo "    --nocolor            Disable colorized output"
    echo "    -p, --proxy=PROXY    Proxy traffic through PROXY"
    echo "    -r, --resume         Resume from a previous run"
    echo "    --simulate           Simulate output but do nothing"
    local default="(default: $threads)"
    echo "    -t, --threads=NUM    Use the specied number of threads"
    echo "                         $default"
    echo "    --timeout=SECS       Use specified timeout with curl"
    echo "                         (default: none)"
    echo "    --tls                Use TLS connection (https)"
    echo "    -v, --verobse        Do not hide parallel errors"
    echo
    exit $1
}

warn() { echo -e "${color:+\e[33m}[-] $@\e[0m"; }

write_resume_file() {
    cat >$json <<EOF
{
  "host": "${host:-${simulate:+simulation}}",
  "simulate": "$simulate",
  "tls": "$tls",
  "wordlists": [
$(
    for i in $(seq 1 ${#wordlists[@]}); do
        echo "    \"${wordlists[$i - 1]}\"${wordlists[$i]:+,}"
    done; unset i
)
  ]
}
EOF
}

# FIXME
echo "Fixing some bugs, check back later"; exit

declare -a args wordlists
unset append dlists help proxy recache resume simulate timeout tls
unset verbose
cache="megabuster.resume"
color="true"
curl="curl -IkLs"
dcache="$HOME/.cache/megabuster/default_list.locations"
joblog="$cache/joblog.txt"
json="$cache/megabuster.json"
threads="32"

# Parse command line options
while [[ $# -gt 0 ]]; do
    case "$1" in
        "--") shift && args+=("$@") && break ;;
        "-a"|"--append") append="true" ;;
        "-c"|"--cache") recache="true" ;;
        "-h"|"--help") help="true" ;;
        "-l"|"--list"*) wordlists+=($(long_opt $@)) || shift ;;
        "--nocolor") unset color ;;
        "-p"|"--proxy"*) proxy="--proxy $(long_opt $@)" || shift ;;
        "-r"|"--resume") resume="--resume" ;;
        "--simulate") simulate="true" ;;
        "-t"|"--threads"*) threads="$(long_opt $@)" || shift ;;
        "--timeout"*)
            arg="$(long_opt $@)" || shift
            timeout="--connect-timeout $arg --max-time $arg"
            ;;
        "--tls") tls="true" ;;
        "-v"|"--verbose") verbose="true" ;;
        *) args+=("$1") ;;
    esac
    shift
done
[[ -z ${args[@]} ]] || set -- "${args[@]}"

# Check for valid params and missing dependencies
[[ -z $help ]] || usage 0
[[ $# -eq 1 ]] || [[ -n $recache$resume$simulate ]] || usage 2
for dep in curl jq parallel; do
    [[ -n $(command -v $dep) ]] || errx 3 "$dep is not installed"
done; unset dep

# Cite if needed
if [[ ! -f $HOME/.parallel/will-cite ]]; then
    mkdir -p $HOME/.parallel
    parallel --citation
fi

# If defaults lists are not cached, find and cache
if [[ ! -f $dcache ]] || [[ -n $recache ]]; then
    cache_default_lists
    [[ -z $recache ]] || exit
fi

# Get default lists
while read -r wordlist; do
    dlists+=($wordlist)
done <$dcache; unset wordlist

# Use default lists if none provided or append was specified
if [[ ${#wordlists[@]} -eq 0 ]] || [[ -n $append ]]; then
    wordlists+=(${dlists[@]})
fi

# Ensure lists are provided and valid
[[ ${#wordlists[@]} -ne 0 ]] || errx 4 "No wordlists provided"
for wordlist in "${wordlists[@]}"; do
    [[ -f $wordlist ]] || errx 5 "Wordlist does not exist: $wordlist"
done; unset wordlist

# Save host
host="${1:-${simulate:+simulation}}"

# Ensure resume directory exists
mkdir -p $cache

# Check for resume file
if [[ -f $json ]] && [[ -z $resume ]]; then
    warn "Resume file found" >&2
    while :; do
        # Prompt whether to overwrite or resume
        read -n 1 -p "Would you like to resume [y/n/q]: " -s ans
        echo

        case "$ans" in
            "n") write_resume_file; break ;;
            "q") exit 0 ;;
            "y") read_resume_file; resume="--resume"; break ;;
            *) echo "Invalid response, try again!" ;;
        esac
    done
elif [[ -f $json ]]; then
    read_resume_file
elif [[ -n $resume ]]; then
    errx 6 "No resume file found in current directory"
else
    write_resume_file
fi

# Display logo
logo >&2

# Script used by parallel
script="/tmp/${0/*\//}.parallel"
trap "clean -1" SIGINT

cat >$script <<EOF
#!/usr/bin/env bash

set -o noglob

check() {
    # Check GET and POST
    for type in GET POST; do
        unset data
        case "\$type" in
            "POST") data="true" ;;
        esac
        while read -r line; do
            case "\$line" in
                "404") continue ;;
            esac
            good "\$type /\$1 (\$line)"
        done < <(
            $curl \${data:+-d {\}} $proxy $timeout -X \$type \\
            "http${tls:+s}://$host/\$1" | \\
            awk '/^(HTTP|Location)/ {print \$2}' | tr -d "\r" | \\
            sed -r "\\\$!N;s/\n/ /"
        ); unset line
    done; unset type
}

# err() { echo -e "\r${color:+\e[31m}[!] \$@\e[0m\e[K"; }

# errx() { echo -e "\r${color:+\e[31m}[!] \${@:2}\e[0m\e[K"; exit \$1; }

good() { echo -e "\r${color:+\e[32m}[+] \$@\e[0m\e[K"; }

# info() { echo -e "\r${color:+\e[37m}[*] \$@\e[0m\e[K"; }

simulate() {
    case "\$((RANDOM % 2))" in
        0) type="GET" ;;
        *) type="POST" ;;
    esac
    case "\$((RANDOM % 3))" in
        0) ret="200" ;;
        1) ret="401" ;;
        *) ret="403" ;;
    esac
    case "\$((RANDOM % 100))" in
        7) good "\$type /\$1 (\$ret)" ;;
    esac
}

# subinfo() { echo -e "\r${color:+\e[36m}[=] \$@\e[0m\e[K"; }

# warn() { echo -e "\r${color:+\e[33m}[-] \$@\e[0m\e[K"; }

# Ignore comments
comment_regex="^( *#.*)?$"
[[ ! "\$1" =~ \$comment_regex ]] || exit

# Fix backslashes and remove leading slashes
path="\${1//\\\/\\\\\\\}"
path="\${path#\${path%%[^/]*}}"

if [[ "x$simulate" != "x" ]]; then
    simulate "\$path"
else
    check "\$path"
fi
EOF
chmod 700 $script

info "MegaBuster is targeting $host"
info "Processing wordlists for ammo"

total="0"
while read -r count; do
    ((total += count))
done < <(grep -cEhvs "^( *#.*)?$" "${wordlists[@]}"); unset count
[[ $total -gt 0 ]] || errx 7 "No ammo"

info "$total shots to fire"

cat ${wordlists[@]} | parallel --bar --joblog ${resume:++}$joblog \
    --lb -P $threads -r $resume $script {}
[[ -n $verbose ]] || echo -en "\e[1A\e[K" >&2

clean
