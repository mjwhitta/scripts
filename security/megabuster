    echo -en "\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e"
    echo -en "[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38"
    echo -en ";5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e"
    echo -en "[48;5;033m\e[38;5;232m▄\e[0m\e[0m\e[48;5;033m\e[38;5;23"
    echo -en "2m▄\e[0m\e[0m\e[48;5;232m\e[38;5;232m▄\e[0m\e[0m"
    echo
    echo
}

long_opt() {
    local arg shift="0"
    case "$1" in
        "--"*"="*) arg="${1#*=}"; [[ -n $arg ]] || usage 1 ;;
        *) shift="1"; shift; [[ $# -gt 0 ]] || usage 1; arg="$1" ;;
    esac
    echo "$arg"
    return $shift
}

read_resume_file() {
    host="$(cat $json | jq -cMrS ".host" | sed "s/null//")"
    rround="$(cat $json | jq -cMrS ".round" | sed "s/null//")"
    simulate="$(cat $json | jq -cMrS ".simulate" | sed "s/null//")"
    tls="$(cat $json | jq -cMrS ".tls" | sed "s/null//")"
}

usage() {
    [[ $1 -ne 0 ]] || logo
    echo "Usage: ${0/*\//} [OPTIONS] <host/ip>[:port][/path][?query]"
    echo
    echo "Simple bash script to enumerate directories and files on a"
    echo "webserver. MegaBuster is like Mega Man, DirBuster and dirb"
    echo "had a baby! By default, MegaBuster will try to use the"
    echo "big.txt and common.txt lists from dirb as well as the"
    echo "directory-list-2.3-medium.txt list from DirBuster."
    echo
    echo "Options:"
    echo "    -a, --apend          Append user specified lists to"
    echo "                         the default"
    echo "    -c, --cache          Cache locations of defaults lists"
    echo "    -h, --help           Display this help message"
    echo "    -l, --list=LIST      Use the specified wordlist (can be"
    echo "                         used more than once)"
    echo "    --nocolor            Disable colorized output"
    echo "    -r, --resume         Resume from a previous run"
    echo "    -s, --simulate       Temporary for testing, simulate"
    echo "                         output but do nothing"
    local default="(default: $threads)"
    echo "    -t, --threads=NUM    Use the specied number of threads"
    echo "                         if running in parallel $default"
    echo "    --timeout=SECS       Use specified timeout with curl"
    echo "                         (default: none)"
    echo "    --tls                Use TLS connection (https)"
    echo
    exit $1
}

warn() { echo -e "${color:+\e[33m}[-] $@\e[0m"; }

write_resume_file() {
    cat >$json <<EOF
{
  "host": "${host:-${simulate:+simulation}}",
  "round": "${1:-0}",
  "simulate": "$simulate",
  "tls": "$tls"
}
EOF
}

declare -a args
unset append dlists help recache resume simulate timeout tls wordlists
cache="megabuster.resume"
color="true"
dcache="$HOME/.cache/megabuster/default_list.locations"
joblog="$cache/joblog.txt"
json="$cache/megabuster.json"
parallel="true"
threads="32"

# Parse command line options
while [[ $# -gt 0 ]]; do
    case "$1" in
        "--") shift && args+=("$@") && break ;;
        "-a"|"--append") append="true" ;;
        "-c"|"--cache") recache="true" ;;
        "-h"|"--help") help="true" ;;
        "-l"|"--list"*) wordlists+=($(long_opt $@)) || shift ;;
        "--nocolor") unset color ;;
        "-r"|"--resume") resume="--resume" ;;
        "-s"|"--simulate") simulate="true" ;;
        "-t"|"--threads"*) threads="$(long_opt $@)" || shift ;;
        "--timeout"*)
            arg="$(long_opt $@)" || shift
            timeout="--connect-timeout $arg --max-time $arg"
            ;;
        "--tls") tls="true" ;;
        *) args+=("$1") ;;
    esac
    shift
done
[[ -z ${args[@]} ]] || set -- "${args[@]}"

# Check for valid params and missing dependencies
[[ -z $help ]] || usage 0
[[ $# -eq 1 ]] || [[ -n $recache$resume$simulate ]] || usage 2
for dep in curl jq; do
    [[ -n $(command -v $dep) ]] || errx 3 "$dep is not installed"
done; unset dep

# If defaults lists are not cached, find and cache
if [[ ! -f $dcache ]] || [[ -n $recache ]]; then
    cache_default_lists
    [[ -z $recache ]] || exit
fi

# Get default lists
while read -r wordlist; do
    dlists+=($wordlist)
done <$dcache; unset wordlist

# Use default lists if none provided or append was specified
if [[ ${#wordlists[@]} -eq 0 ]] || [[ -n $append ]]; then
    wordlists+=(${dlists[@]})
fi

# Ensure lists are provided and valid
[[ ${#wordlists[@]} -ne 0 ]] || err 4 "No wordlists provided"
for wordlist in "${wordlists[@]}"; do
    [[ -f $wordlist ]] || err 5 "Wordlist does not exist: $wordlist"
done; unset wordlist

# Determine parallel situation
if [[ -n $parallel ]] && [[ -z $(command -v parallel) ]]; then
    warn "Parallel is not installed, defaulting to single-threaded"
    unset parallel
fi

# Cite if needed
if [[ -n $parallel ]] && [[ ! -f $HOME/.parallel/will-cite ]]; then
    mkdir -p $HOME/.parallel
    parallel --citation
fi

# Initialize some variables
host="$1"
round="0"

# Resume functionality only works with parallel
if [[ -n $parallel ]]; then
    # Ensure resume directory exists
    mkdir -p $cache

    # Check for resume file
    if [[ -f $json ]] && [[ -z $resume ]]; then
        warn "Resume file found"
        while :; do
            # Prompt whether to overwrite or resume
            read -n 1 -p "Would you like to resume [y/n/q]: " -s ans
            echo

            case "$ans" in
                "n") write_resume_file; break ;;
                "q") exit 0 ;;
                "y") read_resume_file; resume="--resume"; break ;;
                *) echo "Invalid response, try again!" ;;
            esac
        done
    elif [[ -f $json ]]; then
        read_resume_file
    elif [[ -n $resume ]]; then
        err 6 "No resume file found in current directory"
    else
        write_resume_file
    fi
fi

# Display logo
logo

# Script used by parallel
script="/tmp/${0/*\//}.parallel"
trap clean SIGINT

cat >$script <<EOF
#!/usr/bin/env bash

check() {
    data="\$(
        curl $timeout -Iks "http${tls:+s}://$host/\$1" | \\
        awk '/^HTTP\// {print \$2}'
    )"
    if [[ -n \$data ]] &&
       [[ \$(echo "\$data" | head -n 1) -ne 404 ]]
    then
        info "\$1 (\$data)"
    fi
}

err() { echo -e "\r${color:+\e[31m}[!] \$@\e[0m\e[K"; }

errx() { echo -e "\r${color:+\e[31m}[!] \${@:2}\e[0m\e[K"; exit \$1; }

info() { echo -e "\r${color:+\e[32m}[+] \$@\e[0m\e[K"; }

warn() { echo -e "\r${color:+\e[33m}[-] \$@\e[0m\e[K"; }

if [[ "x$simulate" != "x" ]]; then
    let "rand = RANDOM % 50"
    [[ \$rand -ne 7 ]] || info "\$1"
else
    check "\$1"
fi
EOF
chmod 700 $script

info "Processing dataset for jobs"

declare -a dataset=($(
    cat ${wordlists[@]} | grep -Ev "^( *#.*)?$" | sed "s/*//g" | \
    sort -u
))
let "total = ${#dataset[@]}"

[[ $total -ne 0 ]] || errx 7 "No dataset provided"

info "$total jobs to run"
if [[ -n $parallel ]]; then
    # Math
    left="$total"
    marker="0"
    maxdata="$(
        parallel -r --show-limit </dev/null | \
        awk '/Maximal used/ {print $NF; exit}'
    )"
    [[ $maxdata -lt 5000 ]] || let "maxdata -= 5000"
    let "remainder = total % maxdata"
    let "rounds = total / maxdata"
    [[ $remainder -eq 0 ]] || let "rounds += 1"

    # Loop thru rounds
    [[ $rounds -eq 1 ]] || info "Splitting into $rounds rounds"
    while [[ $left -gt 0 ]]; do
        let "round += 1"
        [[ $round == $rround ]] && unset rround

        increment="$left"
        [[ $left -lt $maxdata ]] || let "increment = $maxdata - 1"

        if [[ -z $rround ]]; then
            write_resume_file $round

            [[ $rounds -eq 1 ]] || warn "Round $round"

            parallel --bar --joblog ${resume:++}$joblog --lb \
                -P $threads -r $resume \
                $script {} ::: ${dataset[@]:$marker:$increment}
            unset resume
        fi

        let "left -= increment"
        let "marker += increment"
        [[ -n $rround ]] || echo -en "\e[1A\e[K"
    done
else
    let "count = 1"
    for data in "${dataset[@]}"; do
        echo -e "\r\e[K" >&2
        echo -en "${color:+\e[37m}[$count/$total]\e[0m\e[K\e[1A" >&2
        $script "$data"
        let "count += 1"
    done; unset data
fi

clean
